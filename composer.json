{
    "name": "aih/aih-bundle",
    "description": "Symfony Bundle for AIH",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoan Bernabeu",
            "email": "yoan.bernabeu@alpeshabitat.fr"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "symfony/framework-bundle": "^6.2 || ^7.0 || ^8.0",
        "symfony/config": "^6.2 || ^7.0 || ^8.0",
        "symfony/dependency-injection": "^6.2 || ^7.0 || ^8.0",
        "symfony/http-kernel": "^6.2 || ^7.0 || ^8.0",
        "symfony/http-client": "^6.2 || ^7.0 || ^8.0",
        "microsoft/microsoft-graph": "^1.66",
        "symfony/console": "^6.2 || ^7.0 || ^8.0",
        "doctrine/orm": "^2.0 || ^3.0",
        "aws/aws-sdk-php": "^3.323"
    },
    "autoload": {
        "psr-4": {
            "Aih\\AihBundle\\": "src/"
        }
    },
    "require-dev": {
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-beberlei-assert": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "symfony/framework-bundle": "^6.2 || ^7.0 || ^8.0",
        "symfony/phpunit-bridge": "^6.2 || ^7.0 || ^8.0"
    },
    "scripts": {
        "test:phpstan": "./vendor/bin/phpstan analyse ./src",
        "test:cs": "./vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    }
}
